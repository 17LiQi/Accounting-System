{
  "env": { "browser": true, "es2021": true },
  "extends": ["plugin:vue/vue3-essential", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "ecmaVersion": 12, "sourceType": "module" },
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"]
  }
}